(defvar *emacs-base* (file-name-directory (file-truename user-init-file)))
(defvar *emacs-lib-base* (concat *emacs-base* "libs/"))
(add-to-list 'load-path *emacs-base*)
(add-to-list 'load-path *emacs-lib-base*)

